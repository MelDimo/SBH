<UserControl x:Class="com.sbh.gui.unitsbrowser.ViewTemplate.CurrentItemsTemplate"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:com.sbh.gui.unitsbrowser.ViewTemplate"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel>
            <TextBlock Text="{Binding BaseViewModel.CurrentUnitEx.UnitId}"/>
            <TextBlock Text="{Binding BaseViewModel.CurrentUnitEx.UnitName}"/>
            <TextBlock Text="{Binding BaseViewModel.CurrentUnitEx.BranchName}"/>
            <TextBlock Text="{Binding BaseViewModel.CurrentUnitEx.OrgName}"/>
        </StackPanel>
    </Grid>
</UserControl>
