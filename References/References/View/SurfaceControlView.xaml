<UserControl x:Class="com.sbh.gui.references.View.SurfaceControlView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:com.sbh.gui.references"
             xmlns:viewModel="clr-namespace:com.sbh.gui.references.ViewModel"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <UserControl.DataContext>
        <viewModel:SurfaceControlViewModel/>
    </UserControl.DataContext>
    <ContentControl Content="{Binding CurUserControl}"/>
</UserControl>
