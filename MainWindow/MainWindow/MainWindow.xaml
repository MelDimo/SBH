<Window x:Class="com.sbh.gui.mainwindow.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:com.sbh.gui.mainwindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel>
            <Button Content="Run" Click="Button_Click" Margin="5" HorizontalAlignment="Left"/>
            <Button Content="Cancel" Click="Button_Click" Margin="5" HorizontalAlignment="Left"/>
            <TextBlock Name="tblockOutput"/>
        </StackPanel>
    </Grid>
</Window>
